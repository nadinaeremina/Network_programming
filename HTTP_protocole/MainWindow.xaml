<Window x:Class="HTTP_protocole.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HTTP_protocole"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox x:Name="OutputTB" Margin="5" IsReadOnly="true"></TextBox>
        <TextBox Grid.Row="1" x:Name="RequestTB" Margin="5" ></TextBox>
        <Button Grid.Row="2" x:Name="MakeRequestbutton" Click="MakeRequestbutton_Click" >Make Request</Button>
    </Grid>
</Window>
